{
    "workbench.colorTheme": "Tokyo Night",
    "editor.fontFamily": "'CaskaydiaMono Nerd Font', 'monospace', monospace",
    "editor.fontLigatures": true,
    "editor.fontVariations": false,
    "git.openRepositoryInParentFolders": "always",
    "workbench.colorCustomizations": {
        "[Tokyo Night]": {
            "editor.background": "#0a0e19",
            "sideBar.background": "#0a0e19",
            "activityBar.background": "#0a0e19",
            "panel.background": "#0a0e19",
            "terminal.background": "#0a0e19",
            "tab.activeBackground": "#0a0e19",
            "tab.inactiveBackground": "#0a0e19",
            "titleBar.activeBackground": "#0a0e19",
            "titleBar.inactiveBackground": "#0a0e19",
            "editorGroupHeader.tabsBackground": "#0a0e19",
            "breadcrumb.background": "#0a0e19",
            "statusBar.background": "#0a0e19",
            
        }
    },
    "workbench.settings.applyToAllProfiles": [],
    "javascript.updateImportsOnFileMove.enabled": "never",
    "explorer.confirmDelete": false,
    "html.autoClosingTags": false,
    "workbench.editor.enablePreview": false,
    "github.branchProtection": false,
    "github.gitAuthentication": false,
    "cursor.general.gitGraphIndexing": "enabled",
    "cursor.general.enableShadowWorkspace": true,
    "editor.tabSize": 2,
    "chat.editor.fontSize": 16,
    "editor.fontSize": 18,
    "files.autoSave": "afterDelay",
    "cursor.composer.agentLoopOnLints": false,
    "git.confirmSync": false,
}
